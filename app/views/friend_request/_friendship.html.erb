<div class="container-fluid mt-3">
  <div class="accordion" id="accordionFriendship">
    <div class="accordion-item">
      <h2 class="accordion-header" id="friendship">
        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseFriendship" aria-expanded="true" aria-controls="collapseOne">
          Friendship
        </button>
      </h2>
      <div id="collapseFriendship" class="accordion-collapse" aria-labelledby="friendship" data-bs-parent="#collapseFriendship">
        <div class="accordion-body"> 
          <div class="accordion" id="accordionFriendshipBody">
            <%= render "friend_request/pending" %>
            <%= render "friend_request/requested" %>
            <%= render "friend_request/find_friend" %>          
        </div>
      </div>
    </div>
  </div>
</div>
